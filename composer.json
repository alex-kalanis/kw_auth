{
    "name": "alex-kalanis/kw_auth",
    "description": "Authentication for KWCMS",
    "keywords": [
        "authentication",
        "auth",
        "ldap",
        "file",
        "files",
        "database"
    ],
    "license": "BSD-3-Clause",
    "type": "library",
    "require": {
        "alex-kalanis/kw_auth_sources": ">=3.0 <4",
        "alex-kalanis/kw_paths": ">=3.0 <4",
        "php": ">=7.2.0"
    },
    "require-dev": {
        "ext-openssl": "*",
        "alex-kalanis/kw_address_handler": ">=2.1 <3",
        "alex-kalanis/kw_bans": ">=1.3 <2",
        "alex-kalanis/kw_confs": ">=1.1 <2",
        "alex-kalanis/kw_files": ">=3.2 <4",
        "alex-kalanis/kw_locks": ">=2.0 <3",
        "alex-kalanis/kw_mapper": ">=2.0 <3",
        "alex-kalanis/kw_storage": ">=4.0 <5",
        "friendsofphp/php-cs-fixer": "^3.0",
        "phpstan/phpstan": "^1.0",
        "phpstan/phpstan-phpunit": "^1.0",
        "phpunit/phpunit": ">=7.2 <=9"
    },
    "suggest": {
        "alex-kalanis/kw_mapper": "Mapper for accessing resources in KWCMS"
    },
    "autoload": {
        "psr-4": {
            "kalanis\\kw_auth\\": "php-src/"
        }
    },
    "scripts": {
    },
    "minimum-stability": "dev",
    "prefer-stable": true
